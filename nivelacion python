import random
aleatorio = random.randint(0,10)
valor1 = int(input("Dame el valor1 -> "))
while aleatorio!=valor1:
    if aleatorio>valor1:
     print("el valor debe ser mas grande")
     valor1 = int(input("Dame el valor1 -> "))

    if aleatorio<valor1:
     print("el valor debe ser mas pequeÃ±o")
     valor1 = int(input("Dame el valor1 -> "))

if aleatorio==valor1:
    print("bingo")
